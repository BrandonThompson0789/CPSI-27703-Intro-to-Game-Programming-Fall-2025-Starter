{
  "comment": "JointComponent Examples - Demonstrates all joint types in Box2D v3.x",
  "objects": [
    {
      "comment": "DISTANCE JOINT EXAMPLE - Keeps two objects at a fixed distance",
      "name": "DistanceJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 200,
          "posY": 200,
          "bodyType": "dynamic",
          "fixture": {
            "shape": "box",
            "width": 32,
            "height": 32,
            "density": 1.0
          }
        },
        {
          "type": "JointComponent",
          "jointType": "distance",
          "connectedBody": "DistanceTarget",
          "anchorA": [0, 0],
          "anchorB": [0, 0],
          "length": 100,
          "hertz": 4.0,
          "dampingRatio": 0.5,
          "enableBreaking": true,
          "maxBreakForce": 1000.0,
          "comment": "Spring-like distance joint that will break under 1000N of force"
        }
      ]
    },
    {
      "comment": "REVOLUTE JOINT EXAMPLE - Rotation around a pivot point (like a hinge)",
      "name": "RevoluteJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 400,
          "posY": 200,
          "bodyType": "dynamic",
          "fixture": {
            "shape": "box",
            "width": 64,
            "height": 16,
            "density": 1.0
          }
        },
        {
          "type": "JointComponent",
          "jointType": "revolute",
          "connectedBody": "RevolutePivot",
          "anchorA": [-32, 0],
          "anchorB": [0, 0],
          "enableLimit": true,
          "lowerAngle": -1.57,
          "upperAngle": 1.57,
          "enableMotor": true,
          "motorSpeed": 2.0,
          "maxMotorTorque": 100.0,
          "enableBreaking": true,
          "maxBreakTorque": 500.0,
          "comment": "Rotating beam with angle limits and motor, breaks at 500 Nm"
        }
      ]
    },
    {
      "comment": "PRISMATIC JOINT EXAMPLE - Sliding along an axis (like a piston)",
      "name": "PrismaticJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 600,
          "posY": 200,
          "bodyType": "dynamic",
          "fixture": {
            "shape": "box",
            "width": 32,
            "height": 32,
            "density": 1.0
          }
        },
        {
          "type": "JointComponent",
          "jointType": "prismatic",
          "connectedBody": "PrismaticBase",
          "anchorA": [0, 0],
          "anchorB": [0, 0],
          "axis": [0, 1],
          "enableLimit": true,
          "lowerTranslation": -50,
          "upperTranslation": 50,
          "enableMotor": true,
          "motorSpeed": 10.0,
          "maxMotorForce": 200.0,
          "comment": "Vertical sliding joint with limits and motor (piston-like)"
        }
      ]
    },
    {
      "comment": "WELD JOINT EXAMPLE - Rigid connection (like welding two bodies together)",
      "name": "WeldJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 200,
          "posY": 400,
          "bodyType": "dynamic",
          "fixture": {
            "shape": "box",
            "width": 32,
            "height": 32,
            "density": 1.0
          }
        },
        {
          "type": "JointComponent",
          "jointType": "weld",
          "connectedBody": "WeldTarget",
          "anchorA": [16, 0],
          "anchorB": [-16, 0],
          "hertz": 5.0,
          "dampingRatio": 0.7,
          "enableBreaking": true,
          "maxBreakForce": 800.0,
          "maxBreakTorque": 400.0,
          "comment": "Soft weld joint (with spring properties) that can break"
        }
      ]
    },
    {
      "comment": "WHEEL JOINT EXAMPLE - Suspension system (like a car wheel)",
      "name": "WheelJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 400,
          "posY": 400,
          "bodyType": "dynamic",
          "fixture": {
            "shape": "circle",
            "radius": 20,
            "density": 1.0
          }
        },
        {
          "type": "JointComponent",
          "jointType": "wheel",
          "connectedBody": "CarBody",
          "anchorA": [0, 0],
          "anchorB": [0, 30],
          "axis": [0, 1],
          "hertz": 4.0,
          "dampingRatio": 0.7,
          "enableMotor": true,
          "motorSpeed": 5.0,
          "maxMotorTorque": 200.0,
          "comment": "Wheel with suspension spring and motor for driving"
        }
      ]
    },
    {
      "comment": "MOTOR JOINT EXAMPLE - Controls relative motion between bodies",
      "name": "MotorJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 600,
          "posY": 400,
          "bodyType": "dynamic",
          "fixture": {
            "shape": "box",
            "width": 32,
            "height": 32,
            "density": 1.0
          }
        },
        {
          "type": "JointComponent",
          "jointType": "motor",
          "connectedBody": "MotorTarget",
          "linearOffset": [50, 0],
          "angularOffset": 0.785,
          "maxForce": 100.0,
          "maxTorque": 50.0,
          "comment": "Motor joint maintains relative position and angle offset"
        }
      ]
    },
    {
      "comment": "MOUSE JOINT EXAMPLE - Makes a point track a world position",
      "name": "MouseJointExample",
      "components": [
        {
          "type": "BodyComponent",
          "posX": 200,
          "posY": 600,
          "bodyType": "dynamic",
